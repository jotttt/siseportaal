<table id="wiki_folders_table" class="table table-hover table-condensed">
    <thead>
        <tr>
            <th>{{t.txt_folders}}</th>
        </tr>
    </thead>
    <tbody>
        {{#each wikiFolders}}
        <tr class="folder-links" data-link="{{this.id}}" data-type="{{this.type}}">
            <td class="filename ui-draggable ui-droppable">
                <label>
                    <div class="thumbnail" style="background-image:url(/lemon/plugins/cms/filebrowser/img/filetypes/folder.svg); background-size: 32px;">

                    </div>
                </label>
               <a href="" class="name">
                    <span class="nametext">
                        <span class="innernametext">
                            {{this.name}}
                        </span>

                </a>

            </td>
        </tr>
        {{/each}}
    </tbody>
</table>


