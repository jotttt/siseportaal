<table id="wiki_articles_table" class="table table-hover table-condensed">
    <thead>
        <tr>
            <th>{{t.txt_articles}}</th>
        </tr>
    </thead>
    <tbody>
        {{#each wikiArticles}}
        <tr class="row-links" data-link="{{this.id}}" data-type="{{this.type}}">
            <td  class="filename ui-draggable ui-droppable">

                <div class="row">
                    <div class="col-xs-2">
                        <span class="thumbnail">
                            <i class="fa fa-file-text-o fa-2x"></i>
                        </span>
                    </div>
                    <div class="col-xs-10">
                        <a href="" class="name">
                            <span class="nametext">
                                <span class="innernametext">
                                    {{this.name}}
                                </span>
                            </span>
                        </a>
                    </div>
                </div>

            </td>
        </tr>
        {{/each}}
    </tbody>
</table>

